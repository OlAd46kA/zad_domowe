#zadanie 3
k = int(input())
l = int(input())
m = int(input())
if k == l:
  print("Dwie pierwsze liczby są równe")
else:
  print("pierwsza i ostatnia liczby są równa")
else:
  print("Dwie ostatnie liczby są równe")
else:
  print("Żadna liczba nienjest sobie równa")
  
#zadanie 4
a = int(input())
b = int(input())
c = int(input())
d = int(input())
najmniejsza = a
if b < najmniejsza:
  najmniejsza = b
if c < najmniejsza:
  najmniejsza = c
if d < najmniejsza:
  najmniejsza = d
print(najmniejsza)
  
#zadanie 5
a = int(input())
b = int(input())
c = int(input())
if a > b+c:
  print("Nie")
else:
  if b > a+c:
    print("Nie")
  else:
    if c > a+b:
      print("Nie")
    else:
      print("Tak")
        
#zadanie 6
a = int(input())
b = int(input())
c = int(input())
if c > a and c > b:
  najkrótszy_bok = a
  drugi_najkrótszy_bok = b
  najdłuższy_bok = c
else:
  if b > a and b > c:
    najkrótszy_bok = a
    drugi_najkrótszy_bok = c
    najdłuższy_bok = b
  else:
    if a > b and a > c:
      najkrótszy_bok = b
      drugi_najkrótszy_bok = c
      najdłuższy_bok = a
if 
najkrótszy_bok**2+drugi_najkrótszy_bok**2 > najdłuższy_bok**2:
  print("trójkąt ostrokątny")
else:
  if
najkrótszy_bok**2+drugi_najkrótszy_bok**2 == najdłuższy_bok**2:
  print("trójkąt prostokątny")
else:
  if
najkrótszy_bok**2+drugi_najkrótszy_bok**2 < najdłuższy_bok**2:
  print("trójkąt rozwartokątny") 
